<!DOCTYPE html>
<html lang="en">
  <head>
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta charset="utf-8">
    <title>APIDash</title>

    <link rel="stylesheet" href="/static/jquery-ui.css" media="all" type="text/css" />
    <link rel="stylesheet" href="/static/style.css" media="all" type="text/css" />
    <script type="text/javascript" src="/static/speakClient.js"></script>
  </head>
  <body>
    <div id="audio"></div>

    <div id="left-shower-bar" class="left-hider-shower">
      <div>&#9776;</div>
    </div>

    <div id="left-panel">
      <div id="left-main-panel">
        <div id="left-bar">
          <div id="left-hider" class="left-hider-shower">&#9776;</div>
          <h1>APIdash</h1>
        </div>
      </div>

      <div id="left-med-panel"></div>
      <div id="left-sub-panel">
        <div>
          <button>Forcast</button>
        </div>
      </div>
    </div>

    <div id="right-content">
      <div id="codemodal" title="Module Editor">
        <textarea></textarea>
      </div>
      <div id="container"></div>
      <div id="console"></div>
    </div>

    <script src="static/vendor.js"></script>
    <script src="static/app.js"></script>
    <script>
      $("#codemodal").dialog({autoOpen: false, width: 500, height: 300});
      APIDash.createBuilder('#container');
    </script>

  </body>
</html>
